{
  "name": "takara",
  "version": "1.0.0",
  "main": "index.html",
  "repository": "https://github.com/404missinglink/takaraAiStatic.git",
  "author": "jordan-ia <jordan@i-a.cloud>",
  "license": "MIT",
  "scripts": {
    "start": "concurrently \"npm run watch:tw\" \"npm run watch:hugo\"",
    "watch:tw": "cd env && tailwindcss -i ./assets/css/main.css -o ./assets/css/style.css --watch",
    "watch:hugo": "cd env && hugo server",
    "build": "cd env && hugo --minify",
    "test": "cd env && hugo"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5"
  },
  "dependencies": {
    "@tailwindcss/typography": "^0.5.10",
    "concurrently": "^8.2.2",
    "cssnano": "^6.0.1",
    "cssnano-preset-advanced": "^6.0.1",
    "postcss-cli": "^10.1.0",
    "terser": "^5.24.0"
  }
}
